class Solution {
public:
    int subarraysDivByK(vector<int>& a, int k) {
        int c=0;
        for (int i=0;i<a.size();i++){
            int s=0;
            s+=a[i];
            for(int j=i+1;j<a.size();j++){
                s=s+a[j];
                if (s%k==0){
                    c++;
                }
            }
        }
        return c;
    }
};
